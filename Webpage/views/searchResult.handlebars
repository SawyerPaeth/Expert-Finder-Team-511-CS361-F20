                <li>
                    <a href="https://github.com/SawyerPaeth/Expert-Finder-Team-511-CS361-F20">LinkedIn.com/John-Doe</a>
                </li>
                
            </ul>
        </div>
        <div class="col-sm-3"></div>
        
    </div> 
   
   
    <div class="row">
        <div class="col-sm-3"></div>
        <div class="col-sm-3 border-bottom border-left border-success">
            <ul style="list-style-type:none;">
                <h5 class="font-weight-bold"> Classes: </h5>
                    
                    {{#each Classes}}
                    <li>{{this.description}}</li>
                    {{/each}}
            </ul>
        </div>
        <div class="col-sm-3 border-bottom border-right border-success">
